setlist/:id/edit
<div class="current_list_container">
  <ol>
    <h3>Current Playlist</h3>
  <% @tracks.each do |track| %>
    <div class="song_container">
      <li class="song"><%= track[:name] %></li>
      <h7 class="song_details">
        <span class="artist_name"><%= track[:artist] %></span> |
        <span class="album_name"><%= track[:album] %></span>
      </h7>
    </div>
  <% end %>
  </ol>
</div>
