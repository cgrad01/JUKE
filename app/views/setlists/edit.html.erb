setlist/:id/edit
<div class="current_list_container">
  <ol>
    <h3>Current Playlist</h3>
  <% @tracks.each do |track| %>
    <div class="songs_container">
      <li class="song"><%= track[:title] %></li>
      <h7 class="song_details">
        <span class="artist_name"><%= track[:artist] %></span> |
        <span class="album_name"><%= track[:album] %></span>
      </h7>
    </div>
  <% end %>
  </ol>
  <%# change the above to the render partial (commented out below) when the /juke/new is complete.  This will reduce the number of requests we are making to spotify and simplify the above code %>

  <%# render partial: "show_partial", locals: {list_title: "Current Playlist", setlist_songs: @setlist.setlist_songs.where(list_status: 2)} %>
</div>

<div class="suggested_songs_container">
  <%= render partial: "show_partial", locals: {list_title: "Suggestions", setlist_songs: @setlist.setlist_songs.where(list_status: [0,1])} %>
</div>
